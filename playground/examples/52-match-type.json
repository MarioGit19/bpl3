{
  "order": 52,
  "title": "Type Matching",
  "snippet": "Using match<Type> operator",
  "description": "Demonstrates the match<Type> operator for checking enum variants or types at runtime.",
  "code": [
    "extern printf(fmt: string, ...);",
    "",
    "enum Message {",
    "    Quit,",
    "    Move(int, int),",
    "    Write(string),",
    "}",
    "",
    "frame process(msg: Message) {",
    "    # Check if message is Quit variant",
    "    if (match<Message.Quit>(msg)) {",
    "        printf(\"Quitting\\n\");",
    "        return;",
    "    }",
    "    ",
    "    # Check if message is Move variant",
    "    if (match<Message.Move>(msg)) {",
    "        printf(\"Moving\\n\");",
    "        return;",
    "    }",
    "    ",
    "    printf(\"Other message\\n\");",
    "}",
    "",
    "frame main() ret int {",
    "    process(Message.Quit);",
    "    process(Message.Move(10, 20));",
    "    return 0;",
    "}"
  ]
}
